
{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npx tsx script/build-vercel.ts",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
